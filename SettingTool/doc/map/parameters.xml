			<call_video_time>5</call_video_time>
			<!-- 打电话报警间隔，单位s -->
			<call_intervel>180</call_intervel>
			<!-- 打电话二级报警最低速度 -->
			<call_speed_level2>50</call_speed_level2>
			
			<!-- 吸烟报警 -->
